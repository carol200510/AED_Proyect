<%- include("parcials/header.ejs")%>

<% listaProducto2.forEach((element)=>{%>
  <div class="card mb-3" style="max-width: 540px;">
    <div class="row g-0">
      <div class="col-md-4">
        <img src="/images/herramienta2.jpeg" class="img-fluid rounded-start" alt="Herramienta">
      </div>
      <div class="col-md-8">
        <div class="card-body text-primary">
          <h5 class="card-title">  <%= element['nombreProducto']%></h5>
          <p class="card-text"><%= element['descripcion']%></p>
          <p class="card-text"><small class="text-body-primary">C$ <%= element['precio']%></small></p>
          <button>Compra</button>
        </div>
      </div>
    </div>
  </div>
<%});%>
   


